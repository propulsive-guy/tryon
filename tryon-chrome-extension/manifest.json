{
  "manifest_version": 3,
  "name": "AI Try-On",
  "version": "1.0",
  "description": "Upload a person & cloth image to generate try-on results using Gemini.",
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["http://localhost:5000/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
